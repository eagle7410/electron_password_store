{
  "name": "electron_password_store",
  "version": "1.0.0",
  "description": "App for store passwords.",
  "main": "index-app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "pkg-linux": "electron-packager . --overwrite --platform=linux  --arch=x64  --ignore='/app/|/node_modules/(babel*|gulp*|electron*|grunt*)|/.idea/' --icon=database-migration.ico --prune=true --out=app_build",
    "pkg-win":   "electron-packager . --asar=true --platform=win32  --arch=ia32 --ignore='/app/|/node_modules/(babel*|gulp*|electron*|grunt*)|/.idea/' --icon=database-migration.ico --prune=true --out=app_build -version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Electron migrate creator'",
    "pkg-mac":   "electron-packager . --overwrite --platform=darwin --arch=x64  --ignore='/app/|/node_modules/(babel*|gulp*|electron*|grunt*)|/.idea/' --icon=database-migration.ico --prune=true --out=app_build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eagle7410/electron_password_store.git"
  },
  "keywords": [
    "password",
    "store"
  ],
  "author": {
    "name": "Igor Stcherbina",
    "email": "verycooleagle@gmail.com",
    "skype": "eagleeagle7410"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eagle7410/electron_password_store/issues"
  },
  "devDependencies": {
    "electron-packager": "^8.7.0",
    "electron-prebuilt": "^1.4.13",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-cssmin": "^0.1.7",
    "gulp-uglifyjs": "^0.6.2"
  },
  "homepage": "https://github.com/eagle7410/electron_password_store#readme"
}
